<h1>Listing archers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @archers.each do |archer| %>
      <tr>
        <td><%= archer.name %></td>
        <td><%= link_to 'Show', archer %></td>
        <td><%= link_to 'Edit', edit_archer_path(archer) %></td>
        <td><%= link_to 'Destroy', archer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archer', new_archer_path %>
