<h1>Listing rounds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Face</th>
      <th>Distance</th>
      <th>Total arrows</th>
      <th>Scores</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.name %></td>
        <td><%= round.face %></td>
        <td><%= round.distance %></td>
        <td><%= round.total_arrows %></td>
        <td><%= round.scores %></td>
        <td><%= link_to 'Show', round %></td>
        <td><%= link_to 'Edit', edit_round_path(round) %></td>
        <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round', new_round_path %>
